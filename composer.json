{
    "name" : "zuams/elastic-apm-php-agent",
    "description": "A php agent for Elastic APM v2 Intake API",
    "license": "MIT",
    "version": "1.0.0",
    "require" : {
        "php" : ">= 5.5",
        "guzzlehttp/guzzle": "6.*",
        "ralouphie/getallheaders": "2.*",
        "ext-curl" : "*",
        "ext-json": "*"
    },
    "suggest": {
        "ext-xdebug": "Required for processing of request headers"
    },
    "autoload" : {
        "psr-4" : {
            "Zuams\\": "src/"
        }
    },
    "config" : {
        "optimize-autoloader" : true
    },
